                 left: int[]
                 right: int[]
                }

type char_contents = {
                      left: char[]
                      right: char[]
                     }

let splitPacks( pack: int[]) : contents = 
    // if( pack.Count % 2 != 0) printfn( "ERROR: pack size was not even.  Size = %A", pack.Count) |> unit
    // else unit
    let packHalfSize = pack.Length/2
    { left = pack[0..packHalfSize-1]
      right = pack[packHalfSize..pack.Length]
    }

let char_splitPacks( pack: char[]) : char_contents = 
    // if( pack.Count % 2 != 0) printfn( "ERROR: pack size was not even.  Size = %A", pack.Count) |> unit
    // else unit
    let packHalfSize = pack.Length/2
    { left = pack[0..packHalfSize-1]
      right = pack[packHalfSize..pack.Length]
    }

let backpackContents = backpackArrays
                       |> Array.map (fun x -> splitPacks( x))

let char_backpackContents = char_backpackArrays
                            |> Array.map (fun x -> char_splitPacks( x))


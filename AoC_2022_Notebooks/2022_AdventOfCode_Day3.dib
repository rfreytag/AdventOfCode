#!fsharp

let rec print_groupBy3 (arr: char[][]): unit =
    let first3 = arr[0..2]
    let rest = arr[3..]
    match first3 with 
    | [| x1; x2; x3 |] -> Console.WriteLine( "{0} {1} {2}", x1.Length, x2.Length, x3.Length)
                          print_groupBy3( rest)
    | [||] -> Console.WriteLine( "END")
    | _ -> Console.WriteLine( "ERROR!!!")

// char_backpackArrays |> print_groupBy3

#!fsharp

type triple = {
                first: char[]
                second: char[]
                third: char[] 
               }

let rec groupBy3 (arr: char[][]):  triple[] =
    let first3 = arr[0..2]
    let rest = arr[3..]
    match first3 with 
    | [| x1; x2; x3 |] -> Array.concat [| [| {first = x1; second = x2; third = x3} |]; groupBy3( rest) |]
    | [||] -> Console.WriteLine( "END"); [||]
    | _ -> Console.WriteLine( "ERROR!!!"); [||]

let elvesInGroupsOf3 = char_backpackArrays |> groupBy3

#!fsharp

let badges = [ for g3 in elvesInGroupsOf3 do 
                  Set.intersect( Set.intersect (Set.ofArray g3.first) (Set.ofArray g3.second)) (Set.ofArray g3.third) 
                  |> Set.minElement
                  |> pointValue
              ]

badges


#!fsharp

//let upperCase_points = [65..90]  |> Array.ofSeq
//let lowerCase_points = [97..122] |> Array.ofSeq

//let pointsForLetters = Array.concat [upperCase_points; lowerCase_points]

//let findArrayIndex( elem: int, arr: int[]) :int = Array.FindIndex( arr, (=) elem)

let lowerCase = ['a'..'z'] |> Array.ofSeq
let upperCase = ['A'..'Z'] |> Array.ofSeq

let asciiArray = Array.concat [lowerCase; upperCase]

let findArrayIndex_byChar( elem: char, arr: char[]) :int = Array.FindIndex( arr, (=) elem)

//let pointValue ( c: char) :int = 
//  let charToInt = c |> int
//  let result = findArrayIndex( charToInt, pointsForLetters)
//  result + 1         // adust for 0 indexing

let pointValue ( c: char) :int = findArrayIndex_byChar( c, asciiArray) + 1

//pointValue( 'A') // should equal 52, if char not found value is 0

for c in asciiArray do
  Console.Write( "{0}={1}, ", c, pointValue(c))

#!fsharp

open System.Text.RegularExpressions

let temp = matches |> Seq.map (fun x -> x.Value)
                   |> Array.ofSeq

let backpackArrays = temp 
                     |> Array.map (fun x -> Regex.Matches(x, "(\w)"))
                     |> Array.map (fun y -> y 
                                            |> Array.ofSeq 
                                            |> Array.map (fun z -> z.Value |> char |> pointValue)
                                    )

let char_backpackArrays = temp 
                          |> Array.map (fun x -> Regex.Matches(x, "(\w)"))
                          |> Array.map (fun y -> y 
                                                 |> Array.ofSeq 
                                                 |> Array.map (fun z -> z.Value |> char)
                                        )

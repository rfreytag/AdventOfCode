#!fsharp

let pointsFromPack( pack: contents) : int[] = 
  seq { for point in pack.left do
          pack.right |> Array.fold (fun result x -> if x = point && result = -1 
                                                    then point 
                                                    else result) -1
      } 
  |> Array.ofSeq

#!fsharp

let max_pointsFromPack( pack: contents): int = 
    pointsFromPack( pack) |> Array.fold (fun result x -> if x > result then x else result) -1

let min_pointsFromPack( pack: contents): int = 
    pointsFromPack( pack) |> Array.fold (fun result x -> if x < result then x else result) 100000

#!fsharp

// max_pointsFromPack( test_pack)

let pointsFromEachPack = backpackContents
                         |> Array.map (fun pack -> max_pointsFromPack( pack))

